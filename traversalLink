

import java.awt.*;

import javax.swing.*;

import GraphingIV.cubeUI;

public class traversalLink extends JPanel
{
	traversalCalc t = new traversalCalc();
	boolean first = true;

	public traversalLink()
	{
		setDoubleBuffered(true);
	}

	@Override
	public void paintComponent(Graphics g2)
	{
		g2.setColor(Color.BLACK);
		g2.fillRect(0, 0, this.getWidth() - 198, this.getHeight() - 8);
		if (!t.superSampled)
			t.printVals(g2);
		else
			t.printSuperVals(g2); 
	}

	public static void main(String args[])
	{
		traversalUI tu = new traversalUI();
	}
}
